font_family 		'Fira Code Nerd Font'
bold_font 		auto
italic_font 		auto
bold_italic_font 	auto
font_size 		14.0
tab_bar_edge 		top
background_opacity	0.85
map ctrl+n 		new_window
map ctrl+t 		new_tab
map ctrl+w              close_tab
map ctrl+tab		next_tab
include ./theme.conf



